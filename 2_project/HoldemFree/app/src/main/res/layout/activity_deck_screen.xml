<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/wooden"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="196dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-black"
        android:text="Unlocked skins"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/back_button"
        android:layout_width="64dp"
        android:layout_height="41dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Back"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/winCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="88dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iron"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="148dp"
        app:layout_constraintStart_toEndOf="@+id/wooden"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/golden"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="284dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_button"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/diamond"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginStart="288dp"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iron"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/setWooden"
        android:layout_width="59dp"
        android:layout_height="29dp"
        android:layout_marginStart="176dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Set"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wooden" />

    <Button
        android:id="@+id/setIron"
        android:layout_width="59dp"
        android:layout_height="29dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Set"
        android:textSize="8sp"
        app:layout_constraintStart_toEndOf="@+id/setWooden"
        app:layout_constraintTop_toBottomOf="@+id/iron" />

    <Button
        android:id="@+id/setGolden"
        android:layout_width="59dp"
        android:layout_height="29dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="308dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Set"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/golden" />

    <Button
        android:id="@+id/setDiamond"
        android:layout_width="59dp"
        android:layout_height="29dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Set"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/diamond" />

    <Button
        android:id="@+id/gameButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Game"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/settingsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Settings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/gameButton" />

    <Button
        android:id="@+id/resetButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Reset"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/defaultSkin"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="196dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/setDefault"
        android:layout_width="59dp"
        android:layout_height="29dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_corner_btn"
        android:backgroundTint="#C6C6C6"
        android:text="Set"
        android:textSize="8sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/defaultSkin" />

    <Button
        android:id="@+id/goldenInfo"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="21dp"
        android:background="@android:drawable/ic_dialog_info"
        app:layout_constraintStart_toEndOf="@+id/setGolden"
        app:layout_constraintTop_toBottomOf="@+id/golden" />

    <Button
        android:id="@+id/diamondInfo"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="22dp"
        android:background="@android:drawable/ic_dialog_info"
        app:layout_constraintStart_toEndOf="@+id/setDiamond"
        app:layout_constraintTop_toBottomOf="@+id/diamond" />

    <Button
        android:id="@+id/woodenInfo"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="29dp"
        android:background="@android:drawable/ic_dialog_info"
        app:layout_constraintStart_toEndOf="@+id/setWooden"
        app:layout_constraintTop_toBottomOf="@+id/wooden" />

    <Button
        android:id="@+id/ironInfo"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="29dp"
        android:background="@android:drawable/ic_dialog_info"
        app:layout_constraintStart_toEndOf="@+id/setIron"
        app:layout_constraintTop_toBottomOf="@+id/iron" />
</androidx.constraintlayout.widget.ConstraintLayout>